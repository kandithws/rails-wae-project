<p id="notice"><%= notice %></p>

<h1>Bid Infos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bid_infos.each do |bid_info| %>
      <tr>
        <td><%= link_to 'Show', bid_info %></td>
        <td><%= link_to 'Edit', edit_bid_info_path(bid_info) %></td>
        <td><%= link_to 'Destroy', bid_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bid Info', new_bid_info_path %>
